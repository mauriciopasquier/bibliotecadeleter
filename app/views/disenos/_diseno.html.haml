- cache diseno do
  .row-fluid{class: diseno.senda.downcase}
    .nombre.span3
      = link_to_mostrar [diseno.usuario, diseno], diseno.nombre
    .texto.span4.hidden-phone= diseno.extracto
    .coste.span1.hidden-phone= diseno.coste
    .senda.span1.visible-desktop= diseno.senda
    .tipo.span3.hidden-phone= diseno.linea_de_tipos
