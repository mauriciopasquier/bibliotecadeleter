- cache lista do
  .row-fluid
    .nombre.span4
      - if can? :update, lista
        = link_to_editar edit_usuario_lista_path(@usuario, lista), ''
      - if can? :delete, lista
        = link_to_eliminar [@usuario, lista], ''
      = link_to_mostrar [@usuario, lista], lista.nombre
    .notas.span4= lista.extracto
    .visibilidad.span2= lista.visibilidad
    .cantidad.span2= lista.cantidad
